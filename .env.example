# Configuration de l'API
PORT=3000
NODE_ENV=production

# Clés API (optionnel)
GOOGLE_YT_API_KEY=votre_clé_google_api_ici
APPLE_MUSIC_TOKEN=votre_token_apple_music

# Sécurité
RATE_LIMIT_WINDOW_MS=900000      # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100      # 100 requêtes par fenêtre
JWT_SECRET=votre_secret_jwt_ici

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

# Cache
CACHE_TTL=3600000                # 1 heure
ENABLE_CACHE=true
